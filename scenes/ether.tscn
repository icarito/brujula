[sub_resource type="Shader" id="Shader_note"]
code = "shader_type spatial;
render_mode cull_disabled;

uniform float bend_strength = 0.5;
uniform float width = 0.3;
uniform float wave_amp = 0.003;
uniform float wave_freq = 8.0;
uniform float wave_speed = 1.1;

void vertex() {
	vec3 p = VERTEX;
	if (abs(bend_strength) > 0.01) {
		float half_w = width * 0.5;
		float t = clamp(p.x / half_w, -1.0, 1.0);
		float ang = bend_strength * t;
		float radius = half_w / bend_strength;
		float x_new = sin(ang) * radius;
		float y_curve = (cos(ang) - 1.0) * radius;
		p.x = x_new;
		vec3 model_up = (MODEL_MATRIX * vec4(0.0, 1.0, 0.0, 0.0)).xyz;
		float orientation = sign(model_up.y);
		if (orientation == 0.0) { orientation = 1.0; }
		p.y += y_curve * orientation;
	}
	if (abs(wave_amp) > 0.0001) {
		p.y += sin(TIME * wave_speed + p.x * wave_freq) * wave_amp;
	}
	VERTEX = p;
}

void fragment() {
	ALBEDO = vec3(1.0);
}"

